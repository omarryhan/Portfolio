{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "react-app",
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "no-else-return": "off",
        "no-console": "off",
        "react/no-unescaped-entities": "off"
    }
}