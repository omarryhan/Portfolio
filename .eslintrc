{
    "parser": "@typescript-eslint/parser",
    "env": {
        "browser": true
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "extends": [
        "plugin:@typescript-eslint/recommended",
        "react-app",
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "rules": {
        "indent": "off",
        "@typescript-eslint/indent": ["error", 2],
        "import/prefer-default-export": "off",
        "no-else-return": "off",
        "no-console": "off",
        "react/no-unescaped-entities": "off"
    }
}